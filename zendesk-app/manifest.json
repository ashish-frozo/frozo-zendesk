{
    "name": "EscalateSafe",
    "author": {
        "name": "Frozo",
        "email": "support@frozo.com"
    },
    "version": "0.1.0",
    "frameworkVersion": "2.0",
    "location": {
        "support": {
            "ticket_sidebar": {
                "url": "assets/index.html",
                "flexible": true,
                "size": {
